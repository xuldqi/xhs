# 后端生产环境配置示例
# 复制此文件为 .env.production 并填入实际值

# 环境
NODE_ENV=production
PORT=3000

# Supabase 配置
SUPABASE_URL=https://你的项目ID.supabase.co
SUPABASE_SERVICE_KEY=你的Supabase服务密钥（service_role key）

# AI 服务配置
DASHSCOPE_API_KEY=你的通义千问API密钥

# 支付宝配置（正式环境）
ALIPAY_APP_ID=你的正式应用ID
ALIPAY_PRIVATE_KEY=你的正式私钥
ALIPAY_PUBLIC_KEY=支付宝正式公钥
ALIPAY_GATEWAY=https://openapi.alipay.com/gateway.do
ALIPAY_RETURN_URL=https://你的域名.com/payment/return
ALIPAY_NOTIFY_URL=https://你的后端域名.com/api/payment/notify

# CORS 配置
CORS_ORIGIN=https://你的域名.com

# 日志配置
LOG_LEVEL=info
